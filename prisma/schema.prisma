datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  joyId    Int      @id @default(autoincrement())
  username String
  name     String
  email    String   @unique
  image    String?
  isAdmin  Boolean?
  provider String?
}

generator client {
  provider = "prisma-client-js"
}
